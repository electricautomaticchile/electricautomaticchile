{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/auth/page.tsx",
    "components/footer/footer.tsx",
    "components/navbar/navbat.tsx",
    "components/nosotros/nosotros.tsx",
    "lib/db/db.js",
    "lib/db/db.js",
    "out/types/**/*.ts"
, "../electricautomaticchile-backend/src/controllers/mediciones.controller.ts", "../electricautomaticchile-backend/src/models/Medicion.model.ts", "../electricautomaticchile-backend/src/models/Mensaje.model.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
